{
    "name": "escolalms/notifications",
    "description": "Escola LMS Notifications",
    "type": "package",
    "require": {
        "php": ">=7.4|>=8",
        "laravel/framework": ">=8.0",
        "escolalms/core": "^1",
        "escolalms/templates": "^0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": ">=5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Chołaj",
            "email": "pawel.cholaj@escolasoft.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EscolaLms\\Notifications\\": "src",
            "EscolaLms\\Notifications\\Database\\Seeders\\": "database/seeders",
            "EscolaLms\\Notifications\\Database\\Factories\\": "database/factories",
            "EscolaLms\\Notifications\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EscolaLms\\Notifications\\EscolaLmsNotificationsServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
